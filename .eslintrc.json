{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "no-console": 1, // Means warning
    "prettier/prettier": 1 // Means error
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
